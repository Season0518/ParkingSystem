<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xl.parking.mapper.FeeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xl.parking.domain.Fee">
        <id column="fid" property="fid" />
        <result column="car_type" property="carType" />
        <result column="money" property="money" />
        <result column="money_desc" property="moneyDesc" />
        <result column="fee_time" property="feeTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        fid, car_type, money, money_desc, fee_time
    </sql>

</mapper>
